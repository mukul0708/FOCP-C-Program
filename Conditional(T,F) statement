#include <stdio.h>

int main() {
    int a,b,c,d;
    printf("Enter first number \n");
    scanf("%d",&a);
    printf("Enter second number \n");
    scanf("%d",&b);
    printf("Enter third number \n");
    scanf("%d",&c);
    d=a>b?(a>c?a:c):(b>c?b:c) ;
    printf("%d is the greatest",d);

    return 0;
}
