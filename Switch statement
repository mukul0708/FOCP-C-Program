#include <stdio.h>

int main() {
    int ch,a,b;
    printf("Enter the value of a and b \n");
    scanf("%d%d",&a,&b);
    printf("Enter 1 for add , 2 for sub , 3 for mult , 4 for div \n");
    scanf("%d",&ch);
    switch(ch){
     case 1:{printf("%d",a+b); break;}
     case 2:{printf("%d",a-b); break;}
     case 3:{printf("%d",a*b); break;}
     case 4:{printf("%d",a/b); break;}
     default:{printf("Entered choice is not defined");}
    }
    return 0;
}
